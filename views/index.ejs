<!doctype html>
<html lang="en">

    <head>
        <title>Jokify. Random jokes</title>
        <meta charset="utf-8" />
        <meta name="description" content="Cheer up your day with spanish random jokes" />
        <meta name="author" content="Carles Capellas" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
        <link rel="stylesheet" href="/css/main.css?$modena=jokify" />
        <link rel="icon" type="image/png" href="/favicon.png?$modena=jokify">
    </head>

    <body>
        <div id="viewport" class="fixed-viewport">

        <div id="joke" class="joke">
            <%- joke %>
        </div>

        <button type="button" class="jokifier" id="jokify">
            <span>Jokify</span>
        </button>

        <% for (var i = 1; i <= 50; ++i) { %>
            <span class="symbol trajectory-<%= i %> parabola"></span>
        <% } %>

        </div>

        <script type="text/javascript" src="/plugins/jquery-2.1.1/jquery-2.1.1.min.js?$modena=jokify"></script>
        <script type="text/javascript" src="/js/jokify.js?$modena=jokify"></script>

    </body>
</html>