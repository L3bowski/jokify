{
    "name": "jokify",
    "version": "1.0.0",
    "description": "Cheer up your day with spanish random jokes",
    "main": "index.js",
    "scripts": {
        "build": "npm run clean:build && webpack --config webpack/production.config.js",
        "build:ts": "tsc --noEmit -p tsconfig.json",
        "clean:build": "rm -rf docs",
        "clean:dependencies": "rm -rf node_modules/ package-lock.json",
        "lint": "tslint -e 'node_modules/**' -p .",
        "lint:fix": "tslint -e 'node_modules/**' -p . --fix",
        "prettier": "prettier --list-different \"src/**/*.{ts,tsx}\"",
        "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
        "start": "webpack-dev-server --config webpack/development.config.js"
    },
    "author": "Carles Capellas <capellas.carles@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-swipeable": "5.5.1"
    },
    "devDependencies": {
        "@types/react": "16.9.19",
        "@types/react-dom": "16.9.5",
        "awesome-typescript-loader": "5.2.1",
        "copy-webpack-plugin": "5.1.1",
        "css-loader": "3.4.1",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "husky": "4.0.0",
        "mini-css-extract-plugin": "0.9.0",
        "node-sass": "4.13.0",
        "prerender-spa-plugin": "^3.4.0",
        "prettier": "1.19.1",
        "sass-loader": "8.0.0",
        "tslint": "5.20.1",
        "tslint-config-prettier": "1.18.0",
        "typescript": "3.7.4",
        "webpack": "4.41.5",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.10.1",
        "webpack-merge": "^4.2.2"
    },
    "husky": {
        "pre-commit": "npm run lint && npm run prettier"
    }
}
